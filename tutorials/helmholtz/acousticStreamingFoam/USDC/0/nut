FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(hot-top|graphite|mould|water-film|sonotrode.*|fixed)"
    {
        type            nutkWallFunction;
        value           $internalField;
    }

    ram
    {
        type            calculated;
        value           $internalField;
    }

    free-surface
    {
        type            calculated;
        value           $internalField;
    }

    symmetry
    {
        type		symmetry;
    }
}

