{
    "(hot-top|graphite|mould|water-film|sonotrode-wall|fixed)"
    {
        type            zeroGradient;
    }

    sonotrode
    {
        type            fixedGradient;
        gradient        uniform 15818758852.51479;
    }

    ram
    {
        type            zeroGradient;
    }

    free-surface
    {
        type            fixedValue;
        value           uniform 0;
    }

    symmetric
    {
        type		symmetry;
    }
